    <style name="card_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/card_content</item>
        <item name="android:layout_below">@+id/titleFrame</item>
        <item name="android:layout_toRightOf">@android:id/icon</item>
        <item name="android:layout_marginTop">@dimen/card_content_margin_top</item>
        <item name="android:layout_marginLeft">@dimen/card_title_margin_sides</item>
        <item name="android:layout_marginRight">@dimen/card_title_margin_sides</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="card_header_inner">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_toLeftOf">@android:id/button1</item>
        <item name="android:layout_marginTop">@dimen/card_header_padding_top</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="card_header_outer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/card_header_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/card_header_padding_horizontal</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:background">@drawable/card_header_selector</item>
    </style>

    <style name="card_header_summary">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/card_subheader</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginTop">-3dp</item>
    </style>

    <style name="card_header_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/card_header</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="card_inner_frame">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/card_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/card_margin_horizontal</item>
        <item name="android:layout_marginTop">@dimen/card_margin_vertical</item>
        <item name="android:layout_marginBottom">@dimen/card_margin_vertical</item>
        <item name="android:background">@drawable/card_selector</item>
        <item name="android:paddingTop">@dimen/card_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/card_padding_vertical</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="card_outer_frame">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
    </style>

    <style name="card_popup_anchor">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/card_menu_button</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="card_thumbnail">
        <item name="android:layout_width">@dimen/card_thumbnail</item>
        <item name="android:layout_height">@dimen/card_thumbnail</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:layout_marginRight">@dimen/card_thumbnail_margin</item>
    </style>

    <style name="card_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/card_title</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_toLeftOf">@android:id/button1</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="card_title_compressed" parent="@style/card_title">
        <item name="android:layout_marginLeft">@dimen/card_title_margin_sides</item>
        <item name="android:layout_marginTop">@dimen/card_padding_vertical_taller</item>
        <item name="android:layout_marginBottom">@dimen/card_padding_vertical_taller</item>
    </style>

    <style name="card_title_frame">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">@dimen/card_title_margin_sides</item>
        <item name="android:layout_toRightOf">@android:id/icon</item>
    </style>

    <style name="header_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/card_header_button_text</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:text">@string/see_more</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@color/header_button_default</item>
        <item name="android:layout_marginLeft">@dimen/card_header_button_margin_left</item>
        <item name="android:paddingLeft">@dimen/card_header_button_padding_side</item>
        <item name="android:paddingRight">@dimen/card_header_button_padding_side</item>
        <item name="android:paddingTop">@dimen/card_header_button_padding_top</item>
        <item name="android:paddingBottom">@dimen/card_header_button_padding_top</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginTop">@dimen/card_header_padding_top</item>
    </style>

</resources>
